{
  "java.format.enabled": true,
  "java.format.settings.profile": "GoogleStyle",
  "java.format.onType.enabled": false,
  "java.completion.enabled": true,
  "java.completion.maxResults": 20,
  "java.jdt.ls.vmargs": "-javaagent:/Users/htamakos/lsp/lombok.jar -Xbootclasspath/a:/Users/htamakos/lsp/lombok.jar",
  "java.import.gradle.offline.enabled": false,
  "python.linting.enabled": true,
  "python.linting.mypyEnabled": true,
  "python.linting.pep8Enabled": false,
  "python.formatting.autopep8Path": "/usr/local/anaconda3/bin/autopep8",
  "explorer.icon.enableNerdfont": true,
  "explorer.width": 30,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "ts",
    "typescript",
    "json",
    "rust",
    "go",
    "python"
  ],
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cc", "cpp", "c++", "cuda", "objc", "objcpp"],
      "rootPatterns": [".ccls-root", "compile_commands.json", ".ccls"],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls-cache"
        },
        "client": {
          "snipetSupport": true
        },
        "index": {
          "onChange": true
        },
        "highlight": { "lsRanges": true }
      }
    },
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "settings": {},
      "initializationOptions": {}
    }
  },
  "http.proxy": "jp-proxy.jp.oracle.com:80"
}
